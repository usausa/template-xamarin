<?xml version="1.0" encoding="UTF-8" ?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:sr="clr-namespace:Smart.Forms.Resolver;assembly=Smart.Forms"
             xmlns:shell="clr-namespace:Template.FormsApp.Shell;assembly=Template.FormsApp"
             xmlns:modules="clr-namespace:Template.FormsApp.Modules.Navigation.Shared;assembly=Template.FormsApp"
             x:Class="Template.FormsApp.Modules.Navigation.Shared.SharedMain2View"
             x:DataType="{x:Type modules:SharedMain2ViewModel}"
             sr:BindingContextResolver.Type="{x:Type modules:SharedMain2ViewModel}"
             shell:ShellProperty.Title="SharedMain2"
             shell:ShellProperty.Function1Text="◀️"
             shell:ShellProperty.Function4Text="✔"
             shell:ShellProperty.Function1Enabled="True"
             shell:ShellProperty.Function4Enabled="True">

    <StackLayout Style="{StaticResource FillLayout}">
        <Label Style="{StaticResource LeftValueLabel}"
               Text="{Binding No.Value}" />
    </StackLayout>

</ContentView>
